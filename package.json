{
  "name": "@tsev/social-gateway",
  "version": "1.0.2",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist.index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js",
    "./integrations/*": "./dist/integrations/*/index.js"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "dev": "dotenvx run -- npm run test:watch",
    "lint": "eslint src/**/*.ts",
    "test": "dotenvx run -- vitest --run",
    "test:watch": "dotenvx run -- vitest",
    "test:live": "npm run build && dotenvx run -- vite-node scripts/test",
    "prepare": "husky",
    "prepack": "npm run build"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@dotenvx/dotenvx": "^0.40.0",
    "@eslint/js": "^9.2.0",
    "@inquirer/prompts": "^5.0.4",
    "@types/lodash": "^4.17.4",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.2.0",
    "husky": "^9.0.11",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7.9.0",
    "vite-node": "^1.6.0",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "iso-639-1": "^3.1.2",
    "lodash": "^4.17.21",
    "masto": "^6.7.7",
    "zod": "^3.23.8"
  }
}
